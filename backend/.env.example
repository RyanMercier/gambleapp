# TrendBet Backend Environment Configuration Example
# Copy this file to .env and update the values for your deployment

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/trendbet
SECRET_KEY=your-secret-key-here

# Server Configuration
HOST=0.0.0.0
PORT=8091
PUBLIC_IP=your-server-ip-here
FRONTEND_PORT=4444

# CORS Configuration (comma-separated list of allowed origins)
CORS_ORIGINS=http://localhost:3000,http://localhost:5173,http://127.0.0.1:3000,http://127.0.0.1:5173,http://your-server-ip:5173,http://your-server-ip:3000,http://your-server-ip:4444,http://your-server-ip:8091

# Environment
ENVIRONMENT=development

# Google Trends Configuration
USE_TOR=false